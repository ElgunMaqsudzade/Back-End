@model List<Speaker>
@{
    Layout = null;
}

@foreach (Speaker s in Model)
{
    <div class="single-speaker">
        <div class="speaker-img">
            <img src="~/img/event/@s.Image" alt="speaker">
        </div>
        <div class="speaker-name">
            <h5>@s.Name</h5>
            <p>@s.Profession.Name</p>
        </div>
    </div>
}
